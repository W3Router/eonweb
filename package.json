{
  "name": "eon-protocol-frontend",
  "version": "1.0.0",
  "description": "EON Protocol Frontend",
  "main": "server.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "mocha tests/**/*.test.js --exit",
    "start:original": "node --max-old-space-size=512 server.js",
    "dev:original": "kill -9 $(netstat -tulpn 2>/dev/null | grep ':8080' | awk '{split($7,a,\"/\"); print a[1]}') 2>/dev/null || true && nodemon server.js",
    "test:original": "jest"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.2"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "jest": "^29.7.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
