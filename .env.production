# GCP Project Configuration
PROJECT_ID=your-project-id
REGION=asia-east1

# Database Configuration
DB_HOST=/cloudsql/${PROJECT_ID}:${REGION}:eon-mysql
DB_USER=eonuser
DB_NAME=eon_protocol
CLOUD_SQL_CONNECTION_NAME=${PROJECT_ID}:${REGION}:eon-mysql

# Application Configuration
NODE_ENV=production
PORT=8080

# Security
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=24h

# FHE Module Configuration
FHE_MODULE_URL=http://your-fhe-module-internal-url
FHE_API_KEY=your-fhe-api-key
